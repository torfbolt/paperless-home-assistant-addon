{
  "name": "Paperless-ngx",
  "version": "2.10.0-1",
  "slug": "paperless",
  "url": "https://github.com/paperless-ngx/paperless-ngx",
  "description": "Paperless is an application that manages your personal documents. With the help of a document scanner, paperless transforms your wieldy physical document binders into a searchable archive and provides many utilities for finding and managing your documents.",
  "webui": "http://[HOST]:[PORT:8000]",
  "watchdog": "http://[HOST]:[PORT:8000]",
  "stage": "experimental",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "system",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 8000,
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "8000/tcp": "Web interface (not needed with ingress)"
  },
  "map": ["config", "share:rw", "media:rw"],
  "options": {
    "url": null,
    "directories": {
      "consumption": "/share/paperless/consume",
      "data": "/share/paperless/data",
      "media": "/share/paperless/media"
    },
    "filename": {
      "format": "{created_year}/{correspondent}/{title}"
    },
    "ocr": {
      "language": "eng"
    },
    "default_superuser": {
      "username": null,
      "email": null,
      "password": null
    },
    "timezone": {
      "timezone": "UTC"
    }
  },
  "schema": {
    "url": "str",
    "directories": {
      "consumption": "str",
      "data": "str",
      "media": "str"
    },
    "filename": {
      "format": "str"
    },
    "ocr": {
      "language": "str"
    },
    "default_superuser": {
      "username": "str",
      "email": "email",
      "password": "password"
    },
    "timezone": {
      "timezone": "str"
    }
  }
}
